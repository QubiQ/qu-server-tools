<odoo>
    <record id="import_res_partner_info_form_view" model="ir.ui.view">
        <field name="name">import.res.partner.info.form.view</field>
        <field name="model">res.partner</field>
        <field name="inherit_id" ref="base.view_partner_form"/>
        <field name="arch" type="xml">
            <xpath expr="//page[@name='internal_notes']" position="inside">
                <field name="migration_result" invisible="1"/>
                <field name="migration_description_error" widget="html" readonly="1" attrs="{'invisible': [('migration_result', '!=', 'error')]}"/>
            </xpath>
        </field>
    </record>
</odoo>
